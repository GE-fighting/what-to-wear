  <script>
    // 调试：检查 Alpine.js 是否加载
    document.addEventListener('alpine:init', () => {
      console.log('✅ Alpine.js 已成功加载');
    });
    
    // 筛选侧边栏控制（纯 JavaScript）
    window.addEventListener('DOMContentLoaded', () => {
      console.log('🔍 页面加载完成，初始化筛选侧边栏');
      
      const overlay = document.getElementById('filterOverlay');
      const sidebar = document.getElementById('filterSidebar');
      const openButton = document.querySelector('[\\@click="isFilterOpen = true"]');
      const closeButtons = document.querySelectorAll('[\\@click="isFilterOpen = false"]');
      
      // 确保初始状态是隐藏的
      if (overlay) {
        overlay.style.display = 'none';
        console.log('✅ 遮罩层已隐藏');
      }
      if (sidebar) {
        sidebar.style.display = 'none';
        console.log('✅ 侧边栏已隐藏');
      }
      
      // 打开侧边栏
      function openFilter() {
        console.log('🔓 打开筛选侧边栏');
        if (overlay) {
          overlay.style.display = 'block';
          overlay.style.opacity = '0';
          setTimeout(() => overlay.style.opacity = '1', 10);
        }
        if (sidebar) {
          sidebar.style.display = 'flex';
          sidebar.style.transform = 'translateX(100%)';
          setTimeout(() => sidebar.style.transform = 'translateX(0)', 10);
        }
      }
      
      // 关闭侧边栏
      function closeFilter() {
        console.log('🔒 关闭筛选侧边栏');
        if (overlay) {
          overlay.style.opacity = '0';
          setTimeout(() => overlay.style.display = 'none', 300);
        }
        if (sidebar) {
          sidebar.style.transform = 'translateX(100%)';
          setTimeout(() => sidebar.style.display = 'none', 300);
        }
      }
      
      // 绑定事件
      if (openButton) {
        openButton.addEventListener('click', openFilter);
      }
      
      // 关闭按钮
      const closeBtn = document.getElementById('closeFilterBtn');
      const applyBtn = document.getElementById('applyFilterBtn');
      
      if (closeBtn) {
        closeBtn.addEventListener('click', closeFilter);
      }
      if (applyBtn) {
        applyBtn.addEventListener('click', closeFilter);
      }
      if (overlay) {
        overlay.addEventListener('click', closeFilter);
      }
    });
  </script>
